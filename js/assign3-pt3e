/* JQuery for validation
	displays error message if form input is not a number or empty
	*/

$(document).ready(function () {
	$("#myform").validate({
		// Rules for each input item
		rules: {
			palindrome: {
				required: true,
				number: true,
				minlength: 5,
				maxlength: 5,
			},



		}
	});
});

//function shows palindrome or not, checks using charAt